{


  "name": "iBrowse: Privacy Comparison Extension",
  "description": "Base level Extension to compare and contrast other installed extensions for Google Chrome and Microsoft Edge browsers.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [ "management" , "privacy"], 
  "background":{ 
    "service_worker":"Services/background.js", 
    "type": "module"
  },
  "content_security_policy": {
    "extension_pages": "default-src 'self'"
  }, 

  "web_accessible_resources": [
    {
      "resources": ["src/Assets/*"],
      "matches": ["<all_urls>"]
    }
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Scripts/toogles.js","Scripts/privacy_score.js"]
    }
  ],

"icons": {
"48": "Assets/iBrowse_icon.png"

  }
,
  "action": {
    "default_popup": "iBrowse_page.html", 
    "default_icon": "Assets/iBrowse_icon.png",
    "title": "Security Survey Extension for Google Chrome + Microsoft Edge"
  }
}
